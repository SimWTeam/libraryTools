<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simplewen.win0.wd">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.CloudApp"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/cloud_library_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/cloud_library_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".activity.LoginActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".about" />
        <activity android:name=".activity.CL_Help_Login" />
        <activity android:name=".activity.CloudLibraryMain">

        </activity>
    </application>

</manifest>